CREATE TABLE User (
    Name TEXT,
    Pass TEXT,
    Role TEXT
);

-- Bảng Guest
CREATE TABLE Guest (
    GuestID INTEGER PRIMARY KEY,
    FullName TEXT,
    PhoneNumber TEXT,
    Email TEXT,
    GuestPrivateInf TEXT
);

-- Bảng Booking
CREATE TABLE Booking (
    BookingID INTEGER PRIMARY KEY,
    GuestID INTEGER,
    FullName TEXT,
    TotalPrice REAL,
    Checkin TEXT,
    Checkout TEXT,
    FOREIGN KEY (GuestID) REFERENCES Guest(GuestID)
);

-- Bảng StayPeriod
CREATE TABLE StayPeriod (
    BookingID INTEGER,
    GuestID INTEGER,
    CheckinActual TEXT,
    CheckoutActual TEXT,
    FOREIGN KEY (BookingID) REFERENCES Booking(BookingID),
    FOREIGN KEY (GuestID) REFERENCES Guest(GuestID)
);

-- Bảng RoomInfo
CREATE TABLE RoomInfo (
    RoomID INTEGER PRIMARY KEY,
    RoomType TEXT,
    Capacity INTEGER,
    Description TEXT
);

-- Bảng RoomPrice
CREATE TABLE RoomPrice (
    RoomType TEXT,
    Price REAL,
    StartDate TEXT,
    EndDate TEXT
);

-- Bảng BookingRoom
CREATE TABLE BookingRoom (
    BookingID INTEGER,
    RoomID INTEGER,
    FOREIGN KEY (BookingID) REFERENCES Booking(BookingID),
    FOREIGN KEY (RoomID) REFERENCES RoomInfo(RoomID)
);

-- Bảng RoomInvoice
CREATE TABLE RoomInvoice (
    RoomInvoiceID INTEGER PRIMARY KEY,
    BookingID INTEGER,
    RoomID INTEGER,
    RoomPrice REAL,
    Datetime TEXT,
    FOREIGN KEY (BookingID) REFERENCES Booking(BookingID),
    FOREIGN KEY (RoomID) REFERENCES RoomInfo(RoomID)
);

-- Bảng ServiceInfo
CREATE TABLE ServiceInfo (
    ServiceID INTEGER PRIMARY KEY,
    ServiceName TEXT,
    Descrip TEXT
);

-- Bảng ServicePrice
CREATE TABLE ServicePrice (
    ServiceID INTEGER,
    ServicePrice REAL,
    StartDate TEXT,
    EndDate TEXT,
    FOREIGN KEY (ServiceID) REFERENCES ServiceInfo(ServiceID)
);

-- Bảng ServiceInvoice
CREATE TABLE ServiceInvoice (
    ServiceInvoiceID INTEGER PRIMARY KEY,
    ServicePrice REAL,
    Service INTEGER,
    Datetime TEXT,
    GuestID INTEGER,
    BookingID INTEGER,
    FOREIGN KEY (Service) REFERENCES ServiceInfo(ServiceID),
    FOREIGN KEY (GuestID) REFERENCES Guest(GuestID),
    FOREIGN KEY (BookingID) REFERENCES Booking(BookingID)
);
</sql><current_tab id="0"/></tab_sql></sqlb_project>
